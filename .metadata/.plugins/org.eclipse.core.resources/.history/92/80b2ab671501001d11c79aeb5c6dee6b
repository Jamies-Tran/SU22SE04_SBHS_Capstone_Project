package com.swm.exception;

public class UsernamePasswordNotCorrectException extends RuntimeException {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	public UsernamePasswordNotCorrectException(String userNameOrPassword, String message) {
		super(String.format("[%s]: %s", userNameOrPassword, message));
	
	}
	
	

}
